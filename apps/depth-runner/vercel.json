{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "rewrites": [
      {
        "source": "/assets/(.*)",
        "destination": "/assets/$1"
      },
      {
        "source": "/(.*)\\.(png|jpg)$",
        "destination": "/$1.$2"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }